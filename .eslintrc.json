{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console": [
            0
        ],
        "indent": [
            "error",
            4
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}