{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": [
            "./src/"
        ],
        "exclude": [
            "node_modules",
            "plugins",
            "docs"
        ]
    },
    "opts": {
        "encoding": "utf8",
        "template": "node_modules/docdash",
        "destination": "./public/jsdoc/",
        "recurse": true,
        "private": true
    }
}